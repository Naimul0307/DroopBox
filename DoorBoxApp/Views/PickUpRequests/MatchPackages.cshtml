@model DoorBoxApp.Models.PickUpRequest

@{
    ViewData["Title"] = "Match Packages";
}

@section Css {
    <link asp-append-version="true" type="text/css" rel="stylesheet" href="~/vendor/select2/select2.min.css" />
}

@section Sole {
    <style>


        .table tr th {
            text-align: center;
        }

            .table tr th:first-child {
                width: 160px;
                text-align: left !important;
            }

        .table tr td {
            text-align: center;
        }

            .table tr td:first-child {
                text-align: left;
            }
    </style>
}

<div class="container-fluid mt-4">
    <div class="card mb-4 animated fadeIn">
        <div class="card-header p-3">
            <div class="row align-items-center">
                <div class="col text-center">

                    <h3 class="mb-0 skewed skewed-yellow text-white d-inline-block text-uppercase ls-1">Request Details</h3>


                </div>

            </div>
        </div>
        <div class="card-body p-3">
            <input type="hidden" id="pickUpRequestId" asp-for="Id" />
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">From : @Model.LocationFrom.Name</label>
                        <input type="hidden" id="locationFromId" name="name" value="@Model.LocationFromId" />
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Request Details :@Model.Details</label>

                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Pickup Address : @Model.PickupAddress</label>

                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Merchant Name : @Model.Merchant.Name</label>
                    </div>
                </div>

                <div class="col-md-3">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Requested Package : @Model.TotalPackageNumber Pc(s)</label>
                    </div>
                </div>



                <div class="col-md-3">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Picked Up :@Model.PickedUpPackageNumber Pc(s)</label>

                    </div>
                </div>
                @{
                    int deletedPacks = Model.TotalPackageNumber - Model.Packages.Count;
                }

                <div class="col-md-3">
                    <div class="form-group">
                        @if (deletedPacks > 0)
                        {
                            <label class="form-control-label text-dark ls-1 text-uppercase text-red">Deleted Packages :@Model.PickedUpPackageNumber Pc(s)</label>
                        }
                        else
                        {
                            <label class="form-control-label text-dark ls-1 text-uppercase">Deleted Packages : None</label>
                        }

                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Picked up by : @Model.PickUpDeliveryMan.Name</label>

                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                        <label class="form-control-label text-dark ls-1 text-uppercase">Pickup Date : @String.Format("{0:dd-MM-yyyy}", Model.PickUpDate)</label>

                    </div>
                </div>


            </div>
        </div>
    </div>


    @if (Model.Status == 4)
    {
        <div class="card mb-4 animated fadeIn">
            <div class="card-header p-3">
                <div class="row align-items-center">
                    <div class="col text-center">

                        <h3 class="mb-0 skewed skewed-yellow text-white d-inline-block text-uppercase ls-1">All Packages</h3>


                    </div>

                </div>
            </div>
            <div class="table-responsive">
                <table class="table align-items-center table-hover table-striped table-bordered" id="main_table">
                    <thead class="thead-light">
                        <tr>

                            <th class="text-center">Catagory Type</th>
                            <th class="text-center">Catagory</th>
                            <th class="text-center">Location(To)</th>
                            <th class="text-center">Sub-Location(To)</th>
                            <th class="text-center">Type</th>
                            <th class="text-center">Client</th>
                            <th class="text-center">Address</th>
                            <th class="text-center">Phone</th>
                            <th class="text-center">Details</th>
                            <th class="text-center">Weight</th>
                            <th class="text-center">Price</th>
                            <th class="text-center">Delivary Price</th>
                            <th class="text-center">Selling Price</th>
                            <th class="text-center">Remarks</th>
                            <th class="text-center">Action</th>

                        </tr>
                    </thead>
                    <tbody id="table_body" class="table_body ">
                        @if (Model.Packages.Count != 0)
                        {
                            double productPriceTotal = 0;
                            double deliveryChargeTotal = 0;
                            double weightTotal = 0;
                            int count = 0;
                            @foreach (var package in Model.Packages)
                            {
                                productPriceTotal += (double)package.ProductPrice;
                                deliveryChargeTotal += (double)package.Price;
                                weightTotal += (double)package.Weight;
                                count++;
                                <tr>

                                    <td class="p-0">
                                        <select class="form-control catagoryTypeId js-single border-0 border-radius-0" data-toggle="select">



                                            @if (package.PackageCatagory.CatagoryType != null)
                                            {
                                                @foreach (var catagory in ViewBag.CatagoryTypes)
                                                {

                                                    if (catagory.Id != package.PackageCatagory.CatagoryTypeId)
                                                    {
                                                        <option value="@catagory.Id">@catagory.Name</option>
                                                    }


                                                }
                                                <option selected value="@package.PackageCatagory.CatagoryTypeId">@package.PackageCatagory.CatagoryType.Name</option>
                                            }
                                            else
                                            {
                                                @foreach (var catagory in ViewBag.CatagoryTypes)
                                                {

                                                    <option value="@catagory.Id">@catagory.Name</option>



                                                }
                                                <option selected disabled value="0">Not Selected</option>
                                            }

                                        </select>

                                    </td>



                                    <td class="p-0">
                                        <select class="form-control catagoryId js-single border-0 border-radius-0" data-toggle="select">



                                            @if (package.PackageCatagoryId != null)
                                            {
                                                @foreach (var catagory in ViewBag.Catagories)
                                                {

                                                    if (catagory.CatagoryTypeId == package.PackageCatagory.CatagoryTypeId && catagory.Id != package.PackageCatagoryId)
                                                    {
                                                        <option value="@catagory.Id">@catagory.Name</option>
                                                    }


                                                }
                                                <option selected value="@package.PackageCatagory.Id">@package.PackageCatagory.Name</option>
                                            }

                                        </select>

                                    </td>

                                    <td class="p-0">
                                        <select class="form-control locationToId js-single border-0 border-radius-0 " data-toggle="select">
                                            @if (package.LocationToId != null)
                                            {
                                                @foreach (var location in ViewBag.Locations)
                                                {

                                                    if (location.Id != package.LocationToId)
                                                    {
                                                        <option value="@location.Id">@location.Name</option>
                                                    }


                                                }

                                                <option selected value="@package.LocationTo.Id">@package.LocationTo.Name</option>
                                            }

                                            else
                                            {
                                                @foreach (var location in ViewBag.Locations)
                                                {

                                                    <option value="@location.Id">@location.Name</option>



                                                }
                                                <option class="text-danger" selected disabled value="0">Not Selected</option>
                                            }

                                        </select>

                                    </td>
                                    <td class="p-0">
                                        <select class="form-control subLocationToId js-single border-0 border-radius-0 " data-toggle="select">
                                            @if (package.SubLocationId != null)
                                            {
                                                @foreach (var subLocation in ViewBag.SubLocationId)
                                                {

                                                    if (subLocation.Id != package.SubLocationId && subLocation.LocationId == package.LocationToId)
                                                    {
                                                        <option value="@subLocation.Id">@subLocation.Name</option>
                                                    }


                                                }

                                                <option selected value="@package.SubLocation.Id">@package.SubLocation.Name</option>
                                            }

                                            else
                                            {
                                                @foreach (var subLocation in ViewBag.SubLocationId)
                                                {
                                                    if (subLocation.LocationId == package.LocationToId)
                                                    {
                                                        <option value="@subLocation.Id">@subLocation.Name</option>
                                                    }



                                                }
                                                <option class="text-danger" selected disabled value="0">Not Selected</option>
                                            }

                                        </select>

                                    </td>

                                    <td class="p-0">
                                        <select class="form-control packageTypeId js-single border-0 border-radius-0" data-toggle="select">
                                            @if (package.PackageTypeId != null)
                                            {
                                                @foreach (var type in ViewBag.PackageTypeId)
                                                {

                                                    if (type.Id != package.PackageTypeId)
                                                    {
                                                        <option value="@type.Id">@type.Name</option>
                                                    }


                                                }
                                                <option selected value="@package.PackageType.Id">@package.PackageType.Name</option>
                                            }
                                            else
                                            {
                                                @foreach (var type in ViewBag.PackageTypeId)
                                                {

                                                    <option value="@type.Id">@type.Name</option>



                                                }
                                                <option selected disabled value="0">Not Selected</option>
                                            }

                                        </select>

                                    </td>

                                    <td class="p-0">
                                        <input disabled class="form-control clientName border-0 border-radius-0 text-center" type="text" value="@package.ClientName" />
                                    </td>
                                    <td class="p-0">
                                        <input disabled class="form-control address border-0 border-radius-0 text-center" type="text" value="@package.Address" />
                                    </td>


                                    <td class="p-0">
                                        <input disabled class="form-control phoneNo border-0 border-radius-0 text-center text-wrap" type="text" value="@package.PhoneNumber" />
                                    </td>
                                    <td class="p-0">
                                        <input disabled class="form-control details border-0 border-radius-0 text-center" type="text" value="@package.Details" />
                                    </td>
                                    @if (package.Weight != null)
                                    {
                                        <td class="p-0">
                                            <input class="form-control weight border-0 border-radius-0 text-center" type="text" value="@package.Weight" />
                                        </td>

                                    }
                                    else
                                    {
                                        <td class="p-0">
                                            <input class="form-control weight border-0 border-radius-0 text-center" type="text" value="" />
                                        </td>

                                    }
                                    <td class="p-0">
                                        <input disabled class="form-control productPrice border-0 border-radius-0 text-center" type="text" value="@package.ProductPrice" />
                                    </td>

                                    <td class="p-0">
                                        <input class="form-control delivaryPrice border-0 border-radius-0 text-center" type="text" value="@package.Price" />
                                    </td>


                                    <td class="p-0">
                                        <input disabled class="form-control sellingPrice border-0 border-radius-0 text-center" type="text" value="@package.SellingPrice" />
                                    </td>



                                    <td class="p-0">
                                        <input disabled class="form-control remarks border-0 border-radius-0 text-center" type="text" value="@package.Remarks" />
                                        <input type="hidden" class="packageId" name="packageId" value="@package.Id" />
                                    </td>






                                    @if (package.Status == 1)
                                    {

                                        <td class="text-center align-middle p-2">
                                            <div class="btn-group">

                                                <input class="btn btn-primary btn-sm mr-0 saveButton" type="button" id="noAction" value="Edit" />
                                                <input class="btn btn-danger btn-sm delete_button " type="button" name="delete_button" id="delete_button" value="Delete" />
                                            </div>


                                        </td>
                                    }
                                    else
                                    {
                                        <td class="text-center align-middle p-2">
                                            <div class="btn-group">

                                                <input class="btn btn-primary btn-sm mr-0 saveButton" type="button" id="noAction" value="Re-Save" />
                                                @*<input class="btn btn-danger btn-sm delete_button " type="button" name="delete_button" id="delete_button" value="Delete" />*@
                                            </div>
                                        </td>
                                    }

                                </tr>
                            }

                            <tr>
                                <td class="text-center font-weight-800" colspan="7">Number of Packages : @count</td>
                                <td class="text-center font-weight-800" colspan="2">Total :</td>
                                <td class="text-center font-weight-800">@weightTotal KG</td>
                                <td class="text-center font-weight-800">@productPriceTotal TK</td>
                                <td class="text-center font-weight-800">@deliveryChargeTotal TK</td>
                                <td class="text-center font-weight-800" colspan="3"></td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
            @if (ViewBag.Finalizable)
            {
                <div class="col-12">
                    <div class="text-center">
                        <input class="btn btn-success mt-3 mb-3 " type="button" name="name" id="finalizeButton" value="Finalize">
                    </div>
                </div>
            }

        </div>
    }
    else if (Model.Status == 5)
    {
        <div class="card mb-4 animated fadeIn">
            <div class="card-header p-3">
                <div class="row align-items-center">
                    <div class="col text-center">
                        <h3 class="mb-0 skewed skewed-yellow text-white d-inline-block text-uppercase ls-1">All Packages</h3>
                    </div>

                </div>
            </div>
            <div class="table-responsive">
                <table class="table align-items-center table-hover table-striped table-bordered" id="main_table">
                    <thead class="thead-light">
                        <tr>
                            <th class="text-center">Catagory Type</th>
                            <th class="text-center">Catagory</th>
                            <th class="text-center">Location(To)</th>
                            <th class="text-center">Sub-Location(To)</th>
                            <th class="text-center">Type</th>
                            <th class="text-center">Tracking NO</th>
                            <th class="text-center">Client</th>
                            <th class="text-center">Address</th>
                            <th class="text-center">Phone</th>
                            <th class="text-center">Details</th>
                            <th class="text-center">Weight</th>
                            <th class="text-center">Price</th>
                            <th class="text-center">Delivary Price</th>
                            <th class="text-center">Selling Price</th>
                            <th class="text-center">Remarks</th>
                            <th class="text-center">Status</th>

                        </tr>
                    </thead>
                    <tbody id="table_body" class="table_body ">
                        @if (Model.Packages.Count != 0)
                        {
                            double productPriceTotal = 0;
                            double deliveryChargeTotal = 0;
                            double weightTotal = 0;
                            int count = 0;
                            @foreach (var package in Model.Packages)
                            {
                                productPriceTotal += (double)package.ProductPrice;
                                deliveryChargeTotal += (double)package.Price;
                                weightTotal += (double)package.Weight;
                                count++;
                                <tr>
                                    @if (package.PackageCatagory.CatagoryType != null)
                                    {
                                        <td class="text-center font-weight-800 ">@package.PackageCatagory.CatagoryType.Name</td>


                                    }

                                    else
                                    {
                                        <td class="text-center font-weight-800 ">No Data</td>

                                    }

                                    @if (package.PackageCatagoryId != null)
                                    {
                                        <td class="text-center font-weight-800 ">
                                            @package.PackageCatagory.Name


                                        </td>


                                    }

                                    else
                                    {
                                        <td class="text-center font-weight-800 ">No Data</td>

                                    }



                                    @if (package.LocationToId != null)
                                    {
                                        <td class="text-center font-weight-800 ">@package.LocationTo.Name</td>


                                    }
                                    else
                                    {
                                        <td class="text-center font-weight-800 ">No Data</td>
                                    }
                                    @if (package.SubLocationId != null)
                                    {
                                        <td class="text-center font-weight-800 ">@package.SubLocation.Name</td>


                                    }
                                    else
                                    {
                                        <td class="text-center font-weight-800 ">No Data</td>
                                    }
                                    @if (package.PackageTypeId != null)
                                    {
                                        <td class="text-center font-weight-800 ">@package.PackageType.Name</td>


                                    }
                                    else
                                    {
                                        <td class="text-center font-weight-800 ">No Data</td>
                                    }
                                    @if (package.TrackingNumber != null)
                                    {
                                        <td class="text-center font-weight-800 ">@package.TrackingNumber</td>


                                    }
                                    else
                                    {
                                        <td class="text-center font-weight-800 ">No Data</td>
                                    }
                                    <td class="text-center font-weight-800 ">  <input class=" packageId" type="hidden" value="@package.Id" />@package.ClientName</td>
                                    <td class="text-center font-weight-800 text-wrap">@package.Address</td>
                                    <td class="text-center font-weight-800 ">@package.PhoneNumber</td>
                                    <td class="text-center font-weight-800 text-wrap">@package.Details</td>



                                    @if (package.Weight != null)
                                    {
                                        <td class="text-center font-weight-800">@package.Weight KG</td>

                                    }
                                    else
                                    {
                                        <td class="text-center font-weight-800">No Data</td>

                                    }
                                    <td class="text-center font-weight-800 text-wrap">@package.ProductPrice TK</td>
                                    <td class="text-center font-weight-800 text-wrap">@package.Price TK</td>
                                    <td class="text-center font-weight-800 text-wrap">@package.SellingPrice TK</td>
                                    <td class="text-center font-weight-800 text-wrap">@package.Remarks TK</td>
                                    @if (package.Status == 1)
                                    {
                                        <td class="text-center"> <a class="btn btn-success btn-sm">Processing</a></td>
                                    }
                                    else if (package.Status == 2)
                                    {
                                        <td class="text-center"> <a class="btn btn-success btn-sm">Matched</a></td>
                                    }
                                    @if (package.Status == 3)
                                    {
                                        <td class="text-center"> <a class="btn btn-success btn-sm">DeliveryMan Assigned</a></td>
                                    }
                                    @if (package.Status == 4)
                                    {
                                        <td class="text-center"> <a class="btn btn-success btn-sm">OTP Sent To Client</a></td>
                                    }
                                    @if (package.Status == 5)
                                    {
                                        <td class="text-center"> <a class="btn btn-success btn-sm">Delivered</a></td>
                                    }
                                    @if (package.Status == 13)
                                    {
                                        <td class="text-center">
                                            <a class="btn btn-warning btn-sm">Returned</a>
                                            <input class="btn btn-primary btn-sm mr-0 returnToMerchant" type="button" value="Return to Merchant" />
                                        </td>

                                    }
                                    @if (package.Status == 14)
                                    {
                                        <td class="text-center"> <a class="btn btn-warning btn-sm">Returned to Merchant</a></td>
                                    }
                                    @if (package.Status == 15)
                                    {
                                        <td class="text-center"> <a class="btn btn-greenyellow btn-sm">On Hold</a></td>
                                    }

                                </tr>
                            }

                            <tr>
                                <td class="text-center font-weight-800" colspan="8">Number of Packages : @count</td>
                                <td class="text-center font-weight-800" colspan="2">Total :</td>
                                <td class="text-center font-weight-800">@weightTotal KG</td>
                                <td class="text-center font-weight-800">@productPriceTotal TK</td>
                                <td class="text-center font-weight-800">@deliveryChargeTotal TK</td>
                                @{
                                    double totalTk = productPriceTotal + deliveryChargeTotal;
                                }
                                <td class="text-center font-weight-800" colspan="3"></td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>


        </div>
    }





</div>
@section Scripts {

    <script asp-append-version="true" type="text/javascript" src="~/vendor/select2/select2.min.js"></script>
    <script src="~/vendor/bootstrap-datepicker/bootstrap-datepicker.min.js" asp-append-version="true"></script>

    <script>

        $(document).ready(function () {

            $('.saveButton').on('click', function () {
                var row = $(this).closest('tr');
                var catagoryId = row.find('.catagoryId').val();
                var locationToId = row.find('.locationToId').val();
                var weight = row.find('.weight').val();
                var delivaryPrice = row.find('.delivaryPrice').val();
                var packageId = row.find('.packageId').val();
                var subLocationToId = row.find('.subLocationToId').val();
                var packageTypeId = row.find('.packageTypeId').val();
                if (catagoryId == "" || catagoryId == null || locationToId == "" || locationToId == null || weight == "" || packageId == "" || delivaryPrice == "" || delivaryPrice== "" ) {

                         $.notify({
                             title: 'Prompt!',
                             icon: 'fa fa-times',
                             html: true,
                             message: 'Fill Up All The Fields !!!'
                         },
                             {
                                 type: 'warning',
                                 animate: {
                                     enter: 'animated fadeInDown'
                                 },
                                 placement: {
                                     from: "top",
                                     align: "center"
                                 },
                                 spacing: 10,
                                 z_index: 1031,
                                 delay: 1000,
                                 template: '<div data-notify="container" class="alert alert-dismissible alert-{0} alert-notify" role="alert"><span class="alert-icon" data-notify="icon"></span> <div class="alert-text"</div> <span class="alert-title" data-notify="title">{1}</span> <span data-notify="message">{2}</span></div><button type="button" class="close" data-notify="dismiss" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>'
                             });
                 }
                 else {

                     $(this).val('Saving');
                     $(this).attr("disabled", true);



                     var url = '@Url.Action("UpdateWithDeliveryPrice", "Packages")';
                     $.ajax({
                         type: "POST",
                         dataType: 'json',
                         url: url,
                         data: {
                             catagoryId: catagoryId,
                             locationToId: locationToId,
                             locationToId: locationToId,
                             subLocationToId: subLocationToId,
                             weight: weight,
                             packageId: packageId,
                             delivaryPrice: delivaryPrice,
                         },
                         cache: false,
                         success: function (result) {
                             location.reload();
                         }

                     });

                 }

             });

            $(".weight , .locationToId , .catagoryId").on('change', function () {

                var row = $(this).closest('tr');
                var delivaryPrice = row.find(".delivaryPrice");
                var totalPriceBox = row.find(".totalPrice");
                delivaryPrice.val("");
                totalPriceBox.val("");
                var weight = row.find(".weight").val();
                var productPrice = row.find(".productPrice").val();
                var locationFromId = $("#locationFromId").val();
                var locationToId = row.find('.locationToId').val();
                var catagoryId = row.find('.catagoryId').val();
                if (locationToId != null && catagoryId != null && weight!= "") {
                    var url = '@Url.Action("GetTarrif", "Tarrifs")';
                    $.ajax({
                        type: "POST",
                        dataType: 'json',
                        url: url,
                        data: {
                            catagoryId: catagoryId,
                            locationFromId: locationFromId,
                            locationToId: locationToId,
                            weight: weight,
                        },
                        cache: false,
                        success: function (result) {

                            if (result != null) {

                                var totalPrice = eval(productPrice) + eval(result.totalPrice)

                                delivaryPrice.val(result.totalPrice);
                                totalPriceBox.val(totalPrice);

                            }
                            else{
                                //deliveryChargeLabel.text(" No Tarrif Price Availabe, Contact Door box");
                                //totalPriceLabel.text(" No Tarrif Price Availabe, Contact Door box");
                            }
                        }
                    });
                }
            });
            $('.delete_button').on('click', function () {
                var row = $(this).closest('tr');
                var packageId = row.find('.packageId').val();

                 var url = '@Url.Action("DeletePackage", "Packages")';
                 $.ajax({
                     type: "POST",
                     dataType: 'json',
                     url: url,
                     data: {
                         packageId: packageId,
                     },
                     cache: false,
                     success: function (result) {
                         location.reload();
                     }
                 });

            });
            $(".catagoryTypeId").on('change', function () {
                var row = $(this).closest('tr');
                var catagoryId = row.find('.catagoryId');
                var catagoryTypeId = row.find('.catagoryTypeId').val();


                 var url2 = '@Url.Action("GetCatagoryByType", "Packages")';
                $.ajax({
